<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Assignment3.view.ChatController">
   <children>
      <TextArea fx:id="type" layoutX="38.0" layoutY="331.0" prefHeight="48.0" prefWidth="526.0" promptText="Type here..." />
      <ListView fx:id="chat" layoutX="38.0" layoutY="59.0" pickOnBounds="false" prefHeight="232.0" prefWidth="526.0" />
      <Button fx:id="sendButton" layoutX="521.0" layoutY="298.0" mnemonicParsing="false" onAction="#writeMessage" text="Send" />
      <Label layoutX="365.0" layoutY="24.0" text="status:" textFill="#0000007d" />
      <TextField fx:id="status" editable="false" layoutX="410.0" layoutY="20.0" prefHeight="25.0" prefWidth="69.0" />
      <Button fx:id="logoutButton" layoutX="507.0" layoutY="20.0" mnemonicParsing="false" onAction="#logout" text="Log out" />
   </children>
</AnchorPane>
